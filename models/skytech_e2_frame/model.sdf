<?xml version="1.0" ?>
<sdf version="1.9">
  <model name="skytech_e2_frame">
    <!-- The model is static and won't move -->
    <!--<static>true</static> -->

    <link name='base_link'>
      <velocity_decay>
        <linear>0.0</linear>
        <angular>0.0</angular>
      </velocity_decay>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.008</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.015</iyy>
          <iyz>0</iyz>
          <izz>0.017</izz>
        </inertia>
      </inertial>

      <!-- Collision element -->
      <collision name='base_collision'>
        <pose>0 0 -0.08 0 0 0</pose>
        <geometry>
          <box>
            <size>0.47 0.47 0.23</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <ode>
              <max_vel>100.0</max_vel>
              <min_depth>0.001</min_depth>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <!-- Visual element -->
      <visual name='base_visual'>
        <geometry>
          <mesh>
            <uri>model://skytech_e2_frame/meshes/skyteche2_lr0.05.stl</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>1. 1. 1.</ambient>
          <diffuse>1. 1. 1.</diffuse>
          <specular>1 1 1 1</specular>
          <pbr>
            <metal>
              <metalness>0.1</metalness>
              <roughness>0.5</roughness>
            </metal>
          </pbr>
        </material>
      </visual>
    </link>

    <!-- IMU -->
    <link name='imu_link'>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.15</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00002</iyy>
          <iyz>0</iyz>
          <izz>0.00002</izz>
        </inertia>
      </inertial>
      <sensor name="imu_sensor" type="imu">
        <pose degrees="true">0 0 0 180 0 0</pose>
        <always_on>1</always_on>
        <update_rate>1000.0</update_rate>
      </sensor>
    </link>
    <joint name='imu_joint' type='revolute'>
      <child>imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
      </axis>
    </joint>

    <!-- CAMERA -->
    <include>
      <uri>model://gimbal_small_3d</uri>
      <name>gimbal</name>
      <pose degrees="true">0.27 -0.01 -0.12923 90 0 90</pose>
    </include>
    <joint name="gimbal_joint" type="fixed">
      <parent>base_link</parent>
      <child>gimbal::base_link</child>
      <axis>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
        <xyz>0 0 1</xyz>
      </axis>
      <dynamics>
        <damping>1</damping>
        <friction>1000</friction>
      </dynamics>
    </joint>

    <!-- Downward Facing Lidar -->
    <include>
      <uri>model://downward_radar_sensor</uri>
      <name>alt_lidar</name>
      <pose degrees="true">-0.25 0 0.05 0 0 0</pose>
    </include>

    <joint name="alt_lidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>alt_lidar::radar_link</child>
    </joint>


